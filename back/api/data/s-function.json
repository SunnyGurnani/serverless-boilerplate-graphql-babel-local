{
  "name": "data",
  "runtime": "nodejs4.3",
  "description": "Serverless Lambda function for project: serverless-boilerplate",
  "customName": false,
  "customRole": false,
  "handler": "data/handler.handler",
  "timeout": 6,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {
    "optimize": {
      "exclude": ["aws-sdk"],
      "transforms": [{
        "name": "babelify",
        "opts": {
          "presets": [
            "es2015",
            "stage-0"
          ]
        }
      }]
    }
  },
  "endpoints": [{
    "path": "data",
    "method": "POST",
    "type": "AWS",
    "authorizationType": "none",
    "authorizerFunction": false,
    "apiKeyRequired": false,
    "requestParameters": {},
    "requestTemplates": {
      "application/json": "{\"query\" : $input.json(\"$\")}"
    },
    "responses": {
      "400": {
        "statusCode": "400"
      },
      "default": {
        "statusCode": "200",
        "responseParameters": {},
        "responseModels": {},
        "responseTemplates": {
          "application/json": ""
        }
      }
    }
  }, {
    "path": "schema",
    "method": "GET",
    "type": "AWS",
    "authorizationType": "none",
    "authorizerFunction": false,
    "apiKeyRequired": false,
    "requestParameters": {},
    "requestTemplates": "$${introspectionQueryRequestTemplate}",
    "responses": {
      "400": {
        "statusCode": "400"
      },
      "default": {
        "statusCode": "200",
        "responseParameters": {},
        "responseModels": {},
        "responseTemplates": {
          "application/json": ""
        }
      }
    }
  }],
  "events": [],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}",
    "REDIS_URL": "${REDIS_URL}",
    "NODE_ENV": "${NODE_ENV}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
